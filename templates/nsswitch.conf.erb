# managed by puppet - do not edit
# unless you want to change it every 30 minutes
#

# /etc/nsswitch.conf
#
# An example Name Service Switch config file. This file should be
# sorted with the most-used services at the beginning.
#
# The entry '[NOTFOUND=return]' means that the search for an
# entry should stop if the search in the previous entry turned
# up nothing. Note that if the search failed due to some other reason
# (like no NIS server responding) then the search continues with the
# next entry.
#
# Valid entries include:
#
#  nisplus      Use NIS+ (NIS version 3)
#  nis      Use NIS (NIS version 2), also called YP
#  dns      Use DNS (Domain Name Service)
#  files      Use the local files
#  db      Use the local database (.db) files
#  compat      Use NIS on compat mode
#  hesiod      Use Hesiod for user lookups
#  [NOTFOUND=return]  Stop searching if not found so far
#

# To use db, put the "db" in front of "files" for entries you want to be
# looked up first in the databases


<% if @passwd -%>
passwd:     <%= [*@passwd].join(" ") %>
<% end -%>
<% if @shadow -%>
shadow:     <%= [*@shadow].join(" ") %>
<% end -%>
<% if @group -%>
group:      <%= [*@group].join(" ") %>
<% end -%>
<% if @hosts -%>
hosts:      <%= [*@hosts].join(" ") %>
<% end -%>
<% if @bootparams -%>
bootparams:   <%= [*@bootparams].join(" ") %>
<% end -%>
<% if @ethers -%>
ethers:   <%= [*@ethers].join(" ") %>
<% end -%>
<% if @netmasks -%>
netmasks:   <%= [*@netmasks].join(" ") %>
<% end -%>
<% if @networks -%>
networks:   <%= [*@networks].join(" ") %>
<% end -%>
<% if @protocols -%>
protocols:   <%= [*@protocols].join(" ") %>
<% end -%>
<% if @rpc -%>
rpc:     <%= [*@rpc].join(" ") %>
<% end -%>
<% if @services -%>
services:   <%= [*@services].join(" ") %>
<% end -%>
<% if @netgroup -%>
netgroup:   <%= [*@netgroup].join(" ") %>
<% end -%>
<% if @publickey -%>
publickey:   <%= [*@publickey].join(" ") %>
<% end -%>
<% if @automount -%>
automount:  <%= [*@automount].join(" ") %>
<% end -%>
<% if @aliases -%>
aliases:   <%= [*@aliases].join(" ") %>
<% end -%>
<% if @sudoers -%>
sudoers:   <%= [*@sudoers].join(" ") %>
<% end -%>
<% if @gshadow -%>
gshadow:   <%= [*@gshadow].join(" ") %>
<% end -%>
